{
  "name": "hockey-cms",
  "version": "1.0.0",
  "description": "Headless CMS for the Hockey Team Website",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "dotenv -e .env.local -- next dev",
    "dev:staging": "dotenv -e .env.staging -- next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "payload": "payload",
    "generate:types": "payload generate:types",
    "seed:local": "dotenv -e .env.local -- ts-node src/seed/index.ts",
    "seed:staging": "dotenv -e .env.staging -- ts-node src/seed/index.ts",
    "seed:production": "dotenv -e .env.production -- ts-node src/seed/index.ts"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "^3.0.0",
    "@payloadcms/next": "^3.0.0",
    "@payloadcms/richtext-lexical": "^3.0.0",
    "@payloadcms/ui": "^3.0.0",
    "cloudinary": "^2.7.0",
    "dotenv": "^16.4.5",
    "graphql": "^16.8.1",
    "next": "^15.3.4",
    "payload": "^3.0.0",
    "payload-cloudinary-plugin": "^0.1.14",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.30.1",
    "eslint-config-next": "^14.2.3",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
